{
  "expo": {
    "name": "Metacritic",
    "slug": "Metacritic-App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/metacritic-icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "metacritic",
    "splash": {
      "image": "./assets/metacritic-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/metacritic-adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.vitortte.MetacriticApp"
    },
    "web": {
      "favicon": "./assets/metacritic-favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "fe077c58-9cd8-4a6d-9f24-a287e7140aad"
      }
    }
  }
}
